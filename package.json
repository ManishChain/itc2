  "bugs": {
    "url": "https://github.com/settlemint/bpaas/issues"
  },
  "homepage": "https://settlemint.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/settlemint/bpaas.git"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.2.0",
    "@settlemint/enteth-contracts": "3.30.7",
    "@settlemint/enteth-migration-utils": "3.30.7",
    "@truffle/hdwallet-provider": "1.1.1",
    "dayjs": "1.9.5",
    "dotenv": "8.2.0",
    "faker": "5.1.0",
    "ipfs-http-client": "47.0.0",
    "ts-node": "9.0.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/truffle-v5": "3.0.0",
    "@types/faker": "5.1.4",
    "@types/mocha": "8.0.3",
    "@types/node": "14.14.6",
    "eslint": "7.12.1",
    "hardhat": "^2.0.2",
    "hardhat-typechain": "^0.3.3",
    "prettier": "2.1.2",
    "prettier-plugin-solidity": "1.0.0-alpha.59",
    "solhint": "3.3.1",
    "solhint-plugin-prettier": "0.0.5",
    "truffle": "5.1.51",
    "truffle-typings": "1.0.8",
    "ts-generator": "^0.1.1",
    "typechain": "^4.0.0",
    "typescript": "4.0.5",
    "web3": "^1.3.0"
  }
